let int a = 9;

/* el lenguaje es sensible a las may√∫sculas */
function int Void(int a) {
    do {
        if (a == a)
            return a;
    } while (!!!(a < a));

    return a + -a;
}

/* el lenguaje admite recursividad */
function int recursive(int a, float b, string c) {
    return 3 + recursive(
        3 * (implicit + recursive(6,6.0,"6")),
        0.00000000000 * 1.1 * 2.2,
        "."
    );
}
